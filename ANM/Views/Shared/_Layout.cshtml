<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ANM</title>
    @*@Styles.Render("~/Content/css")*@
    @*@Scripts.Render("~/bundles/modernizr")*@
    <!--[if lt IE 11]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
    <link rel="icon" href="~/Content/assets/images/favicon.ico" type="image/x-icon">

    <link rel="stylesheet" href="~/Content/assets/css/style.css">
    <base href="@ANM.Models.CommonModel.GetBaseUrl()" />
</head>
<body>
    <div class="loader-bg">
            <div class="loader-track">
                <div class="loader-fill"></div>
            </div>
        </div>

    @Html.Partial("_Navbar")
    @Html.Partial("_Header")

    <div class="pcoded-main-container">
        @RenderBody()
        <hr />
        <footer>
            <p style="color:#000000 !important; text-align: center !important">Copyright @DateTime.Now.Year ANM. All rights reserved.</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <script src="~/Content/assets/js/plugins/popper.min.js" type="45ed71f3f759f6ec4fcaa4e4-text/javascript"></script>
    <script src="~/Content/assets/js/plugins/perfect-scrollbar.min.js" type="45ed71f3f759f6ec4fcaa4e4-text/javascript"></script>
    <script src="~/Content/assets/js/plugins/bootstrap.min.js" type="45ed71f3f759f6ec4fcaa4e4-text/javascript"></script>
    <script src="~/Content/assets/js/pcoded.js" type="45ed71f3f759f6ec4fcaa4e4-text/javascript"></script>
    <script src="~/Content/assets/js/pages/dashboard-main.js" type="45ed71f3f759f6ec4fcaa4e4-text/javascript"></script>

    @*<script src="~/Content/assets/js/plugins/apexcharts.min.js" type="45ed71f3f759f6ec4fcaa4e4-text/javascript"></script>*@

    <script src="~/Content/assets/js/rocket-loader.min.js" data-cf-settings="45ed71f3f759f6ec4fcaa4e4-|49" defer></script>
    @*

    *@

</body>
</html>
